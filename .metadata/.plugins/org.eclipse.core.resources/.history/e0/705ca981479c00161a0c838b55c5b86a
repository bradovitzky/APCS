package src;

public class Person
{
	String name;
	int age;
	public getName()
	{
		System.out.println(name);
	}
	public Person(String name, int age)
	{
		this.name = name;
		this.age = age;
	}
	public Person()
	{
		name = null;
		age = 0;
	}
	public String toString()
	{
		return name + " is " + age + " years old";
	}
}