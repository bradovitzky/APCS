package textExcel2;

import java.util.*;

public abstract class Cell
{
	// instance variable
	private String contents;
	
	// Constructors start
	public Cell()
	{
		contents = "";
	}
	public Cell(String input)
	{
		contents = input;
	}
	// Constructors end
	
	public void setValue(String input)
	{
		contents = input;
	}
	public String getValue()
	{
		return contents;
	}
	public String toString()
	{
		return contents;
	}
}
