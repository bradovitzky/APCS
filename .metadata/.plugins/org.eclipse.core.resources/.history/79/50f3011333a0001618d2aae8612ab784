package Pets;

public class LoudDog extends Dog
{
	public LoudDog(String name)
	{
		super(name);
	}
	
	public String getName(LoudDog dog)
	{
		return dog.name;
	}
	public String speak()
	{
		return super.speak() + " " + super.speak();
	}
	
	public static void main(String[] args)
	{
		LoudDog loudDoggo = new LoudDog("Loud Doggo");
		System.out.println(loudDoggo);
		System.out.println(loudDoggo + " says " + loudDoggo.speak());
	}

}
