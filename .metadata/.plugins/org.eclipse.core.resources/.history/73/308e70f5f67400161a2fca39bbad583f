package karel;

import kareltherobot.*;

// This robot task is from page 11 in the text
public class CompleteProgram extends UrRobot
{

	public void putFive()
	{
		putBeeper();
		move();
		putBeeper();
		move();
		putBeeper();
		move();
		putBeeper();
		move();
		putBeeper();
		move();
	}
	public void drawB()
	{
		putFive();
	}
	public static void main(String[] args)
	{
		// World file (leave out the readWorld line if you just want a blank world)
		World.readWorld("WorldFiles/fig2-3.kwld");
		World.setVisible(true);
		World.setTrace(true);
		World.setDelay(20);
		
		UrRobot karel = new UrRobot(1,2,East, 13);
		karel.move();
		karel.move();
		karel.pickBeeper();
		karel.turnLeft();
		karel.move();
		karel.drawB();

	}

}
