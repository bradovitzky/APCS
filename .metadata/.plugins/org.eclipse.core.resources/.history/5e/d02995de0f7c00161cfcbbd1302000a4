package karel;

import kareltherobot.*;

public class FencePlanter extends Robot
{
	public FencePlanter(int street, int avenue, Direction dir, int beeps)
    {
        super(street, avenue, dir, beeps);
    }
	
	public static void main(String [] args)
    {
        World.readWorld("WorldFiles/fig3-1.kwld");
        World.setVisible(true);

        FencePlanter Felix = new FencePlanter(1, 1, East, 0);
        World.setDelay(3);
    }
}
